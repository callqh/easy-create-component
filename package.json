  "scripts": {
    "prepublishOnly": "npm run build",
    "dev": "esno src/commands/ni.ts",
    "build": "unbuild",
    "stub": "unbuild --stub",
    "release": "bumpp && npm publish",
    "lint": "eslint .",
    "test": "vitest"
  },
  "dependencies": {
    "consola": "^2.15.3",
    "fs-extra": "^10.1.0",
    "sade": "^1.8.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@types/fs-extra": "^9.0.13",
    "@types/ini": "^1.3.31",
    "@types/node": "^18.0.4",
    "@types/prompts": "^2.4.0",
    "@types/which": "^2.0.1",
    "bumpp": "^8.2.1",
    "eslint": "^8.19.0",
    "prompts": "^2.4.2",
    "typescript": "^4.7.4",
    "unbuild": "^0.7.4",
    "vitest": "^0.18.0",
    "which": "^2.0.2"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
